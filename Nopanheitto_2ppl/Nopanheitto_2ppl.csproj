<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
  </PropertyGroup>

  <ItemGroup>
    <Compile Update="DiceFaces\DiceFaces.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>DiceFaces.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="DiceFaces\DiceFaces.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>DiceFaces.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>